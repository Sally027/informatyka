	cout << "literacja petli: " << i << endl;
	}
	//zagniezdzanie petli
	for(int i =0; i <5;i++){
		for (int j = 0; j < 5; j++){
			cout<< "i:" << i;
			cout << "j:" << i;
		}
	}

	cout << endl;
	system("pouse");
	
}
